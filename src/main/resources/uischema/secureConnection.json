{
  "connectorName": "kafkaTransport",
  "connectionName": "kafkaSecure",
  "title": "Kafka Secured Connection",
  "help": "<h1>Kafka Connector</h1> <b>The Kafka connector allows you to access the Kafka Producer API through WSO2 EI and acts as a message producer that facilitates message publishing. The Kafka connector sends messages to the Kafka brokers.</b>",
  "elements": [
    {
      "type": "attribute",
      "value": {
        "name": "connectionName",
        "displayName": "Connection Name",
        "inputType": "string",
        "defaultValue": "KAFKA_CONNECTION_1",
        "required": "true",
        "helpTip": "The name for the kafka connection",
        "validation": "nameWithoutSpecialCharactors"
      }
    },
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "General",
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "bootstrapServers",
              "displayName": "Bootstrap Servers",
              "inputType": "stringOrExpression",
              "defaultValue": "localhost:9092",
              "required": "true",
              "helpTip": "The Kafka brokers listed in the form host1:port1,host2:port2,..."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "keySerializerClass",
              "displayName": "Key Serializer Class",
              "inputType": "stringOrExpression",
              "defaultValue": "org.apache.kafka.common.serialization.StringSerializer",
              "required": "true",
              "helpTip": "The serializer class for the key that implements the serializer interface."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "valueSerializerClass",
              "displayName": "Value Serializer Class",
              "inputType": "stringOrExpression",
              "defaultValue": "org.apache.kafka.common.serialization.StringSerializer",
              "required": "true",
              "helpTip": "The serializer class for the value that implements the serializer interface."
            }
          }
        ]
      }
    },
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "Security",
        "isCollapsed": true,
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "securityProtocol",
              "displayName": "Security Protocol",
              "inputType": "combo",
              "defaultValue": "PLAINTEXT",
              "required": "false",
              "helpTip":"The protocol used to communicate with brokers.",
              "comboValues": [
                "PLAINTEXT",
                "SSL",
                "SASL_PLAINTEXT",
                "SASL_SSL"
              ]
            }
          },
          {
            "type": "attributeGroup",
            "value": {
              "groupName": "SSL Configurations",
              "isCollapsed": true,
              "enableCondition": [
                "NOT",
                [
                  "OR",
                  {
                    "securityProtocol": "PLAINTEXT"
                  },
                  {
                    "securityProtocol": "SASL_PLAINTEXT"
                  }
                ]
              ],
              "elements": [
                {
                  "type": "attributeGroup",
                  "value": {
                    "groupName": "Trust Store Configurations",
                    "isCollapsed": true,
                    "elements": [
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslTruststoreLocation",
                          "displayName": "SSL Truststore Location",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "The location of the trust store file."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslTruststorePassword",
                          "displayName": "SSL Truststore Password",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "The password for the trust store file."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslTruststoreType",
                          "displayName": "SSL Truststore Type",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The format of the trust store file."
                        }
                      }
                    ]
                  }
                },
                {
                  "type": "attributeGroup",
                  "value": {
                    "groupName": "Key Store Configurations",
                    "isCollapsed": true,
                    "elements": [
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslKeystoreLocation",
                          "displayName": "SSL Keystore Location",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "The location of the key store file. Setting this for the client is optional. Set this when you want to have two-way authentication for the client."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslKeystorePassword",
                          "displayName": "SSL Keystore Password",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "The store password for the keystore file. Setting this for the client is optional. Set it only if ssl.keystore.location is configured."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslKeystoreType",
                          "displayName": "SSL Keystore Type",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The format of the keystore file. Setting this for the client is optional."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslKeyPassword",
                          "displayName": "SSL Key Password",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip": "The password of the private key in the keystore file. Setting this for the client is optional."
                        }
                      }
                    ]
                  }
                },
                {
                  "type": "attributeGroup",
                  "value": {
                    "groupName": "Advanced SSL Configurations",
                    "isCollapsed": true,
                    "elements": [
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslEnabledProtocols",
                          "displayName": "SSL Enabled Protocols",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The list of protocols enabled for SSL connections."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslCipherSuites",
                          "displayName": "SSL Cipher Suites",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"A list of cipher suites."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslProtocol",
                          "displayName": "SSL Protocol",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The SSL protocol used to generate the SSLContext."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslProvider",
                          "displayName": "SSL Provider",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The name of the security provider used for SSL connections. The default value is the default security provider of the JVM."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslEndpointIdentificationAlgorithm",
                          "displayName": "SSL Endpoint Identification Algorithm",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The endpoint identification algorithm to validate the server hostname using a server certificate."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslKeymanagerAlgorithm",
                          "displayName": "SSL Key Manager Algorithm",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The algorithm used by the key manager factory for SSL connections. The default value is the key manager factory algorithm configured for the Java Virtual Machine."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslTrustmanagerAlgorithm",
                          "displayName": "SSL Trust Manager Algorithm",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The algorithm used by the trust manager factory for SSL connections. The default value is the trust manager factory algorithm configured for the Java Virtual Machine."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "sslSecureRandomImplementation",
                          "displayName": "SSL Secure Random Implementation",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The SecureRandom PRNG implementation to use for SSL cryptography operations."
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "type": "attributeGroup",
            "value": {
              "groupName": "SASL Configurations",
              "isCollapsed": true,
              "enableCondition": [
                "NOT",
                [
                  "OR",
                  {
                    "securityProtocol": "PLAINTEXT"
                  },
                  {
                    "securityProtocol": "SSL"
                  }
                ]
              ],
              "elements": [
                {
                  "type": "attribute",
                  "value": {
                    "name": "saslMechanism",
                    "displayName": "SASL Mechanism",
                    "inputType": "combo",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip":"The SASL mechanism to use for authentication.",
                    "comboValues": [
                      "PLAIN",
                      "SCRAM-SHA-256",
                      "SCRAM-SHA-512",
                      "GSSAPI",
                      "OAUTHBEARER"
                    ]
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "saslJaasConfig",
                    "displayName": "SASL Jaas Config",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip":"JAAS login context parameters for SASL connections in the format used by JAAS configuration files."
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "saslLoginCallbackHandlerClass",
                    "displayName": "SASL Login Callback Handler Class",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip":"The fully qualified name of a SASL login callback handler class that implements the AuthenticateCallbackHandler interface."
                  }
                },
                {
                  "type": "attributeGroup",
                  "value": {
                    "groupName": "SASL/GSSAPI (Kerberos)",
                    "elements": [
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslKerberosServiceName",
                          "displayName": "SASL Kerberos Service Name",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The Kerberos principal name that Kafka runs as."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslKerberosKinitCmd",
                          "displayName": "SASL Kerberos Kinit Command Path",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The kerberos kinit command path."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslKerberosMinTimeBeforeRelogin",
                          "displayName": "SASL Kerberos Min Time Before Relogin",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"Login thread's sleep time, in milliseconds, between refresh attempts."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslKerberosTicketRenewJitter",
                          "displayName": "SASL Kerberos Ticket Renew Jitter",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"Percentage of random jitter added to the renewal time."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslKerberosTicketRenewWindowFactor",
                          "displayName": "SASL Kerberos Ticket Renew Window Factor",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The login thread sleeps until the specified window factor of time from the last refresh to the ticket's expiry is reached, after which it will try to renew the ticket."
                        }
                      }
                    ],
                    "enableCondition": [
                      {
                        "saslMechanism": "GSSAPI"
                      }
                    ]
                  }
                },
                {
                  "type": "attributeGroup",
                  "value": {
                    "groupName": "SASL/OAUTHBEARER",
                    "elements": [
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslOauthbearerTokenEndpointUrl",
                          "displayName": "SASL OAuthBearer Token Endpoint URL",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The URL for the OAuth/OIDC identity provider."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslOauthbearerScopeClaimName",
                          "displayName": "SASL OAuthBearer Scope Claim Name",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The override name of the scope claim."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslLoginConnectTimeout",
                          "displayName": "SASL Login Connect Timeout",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The duration, in milliseconds, for HTTPS connect timeout."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslLoginReadTimeout",
                          "displayName": "SASL Login Read Timeout",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The duration, in milliseconds, for HTTPS read timeout."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslLoginRetryBackoff",
                          "displayName": "SASL Login Retry Backoff",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The duration, in milliseconds, to wait between HTTPS call attempts."
                        }
                      },
                      {
                        "type": "attribute",
                        "value": {
                          "name": "saslLoginRetryBackoffMax",
                          "displayName": "SASL Login Retry Backoff Max Time",
                          "inputType": "stringOrExpression",
                          "defaultValue": "",
                          "required": "false",
                          "helpTip":"The maximum duration, in milliseconds, for HTTPS call attempts."
                        }
                      }
                    ],
                    "enableCondition": [
                      {
                        "saslMechanism": "OAUTHBEARER"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    },
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "Advanced",
        "isCollapsed": true,
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "schemaRegistryUrl",
              "displayName": "Schema Registry URL",
              "inputType": "stringOrExpression",
              "defaultValue": "http://localhost:8081",
              "required": "false",
              "helpTip": "The URL of the Confluent Schema Registry."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "basicAuthCredentialsSource",
              "displayName": "Basic Authorization Credentials Source",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip": "The credentials source, if the confluent Schema Registry is secured with basic authorization. "
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "basicAuthUserInfo",
              "displayName": "Basic Authorization Credentials",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip": "The credentials for Confluent Schema Registry URL, if basicAuthCredentialsSource is set to USER_INFO. "
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "keySubjectNameStrategy",
              "displayName": "Key Subject Name Strategy",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The strategy for determining the subject name under which the key schema is registered."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "subjectNameStrategy",
              "displayName": "Value Subject Name Strategy",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The strategy for determining the subject name under which the value schema is registered."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "autoRegisterSchemas",
              "displayName": "Auto Register Schemas",
              "inputType": "boolean",
              "defaultValue": "",
              "required": "false",
              "helpTip":"Serializer will/will not attempt to register new schemas."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "useLatestVersion",
              "displayName": "Use Latest Schema Version",
              "inputType": "boolean",
              "defaultValue": "",
              "required": "false",
              "helpTip":"Serializer retrieve the latest schema version for the subject."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "bufferMemory",
              "displayName": "Buffer Memory (Bytes)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip": "The total bytes of memory the producer can use to buffer records waiting to be sent to the server."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "compressionType",
              "displayName": "Compression Type",
              "inputType": "combo",
              "defaultValue": "",
              "required": "false",
              "helpTip": "The compression type for the data generated by the producer.",
              "comboValues": [
                "none",
                "gzip",
                "snappy",
                "lz4",
                "zstd"
              ]
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "retries",
              "displayName": "Retries",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip": "Set a value greater than zero if you want the client to resent any records automatically when a request fails."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "batchSize",
              "displayName": "Batch Size (Bytes)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip": "Specify how many records the producer should batch together when multiple records are sent to the same partition."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "clientId",
              "displayName": "Client ID",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip": "The client identifier that you pass to the server when making requests."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "connectionsMaxIdleTime",
              "displayName": "Max Idle Time (ms)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip": "The duration in milliseconds after which idle connections should be closed."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "lingerTime",
              "displayName": "Linger Time (ms)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The time, in milliseconds, to wait before sending a record. Set this property when you want the client to reduce the number of requests sent when the load is moderate. This adds a small delay rather than immediately sending out a record. Therefore, the producer waits up to allow other records to be sent so that the requests can be batched together."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "maxBlockTime",
              "displayName": "Max Block Time (ms)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The maximum time in milliseconds that the KafkaProducer.send() and the KafkaProducer.partitionsFor() methods can be blocked."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "maxRequestSize",
              "displayName": "Max Request Size (Bytes)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The maximum size of a request in bytes."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "partitionerClass",
              "displayName": "Partitioner Class",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The partitioner class that implements the partitioner interface."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "sendBufferBytes",
              "displayName": "Send Buffer Bytes (Bytes)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The size of the TCP send buffer (SO_SNDBUF) to use when sending data."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "receiveBufferBytes",
              "displayName": "Receive Buffer Bytes (Bytes)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The size of the TCP receive buffer (SO_RCVBUF) to use when reading data."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "requestTimeout",
              "displayName": "Request Timeout",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The maximum amount of time, in milliseconds, that a client waits for the server to respond."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "acks",
              "displayName": "Acknowledgements",
              "inputType": "combo",
              "defaultValue": "",
              "required": "false",
              "helpTip": "The number of acknowledgments that the producer requires for the leader to receive before considering a request to be complete.",
              "comboValues": [
                "all",
                "-1",
                "0",
                "1"
              ]
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "enableIdempotence",
              "displayName": "Enable Idempotence",
              "inputType": "boolean",
              "defaultValue": "true",
              "required": "false",
              "helpTip":"When set to ‘true’, the producer will ensure that exactly one copy of each message is written in the stream. If ‘false’, producer retries due to broker failures, etc., may write duplicates of the retried message in the stream."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "maxInFlightRequestsPerConnection",
              "displayName": "Max In Flight Requests Per Connection",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The maximum number of unacknowledged requests that the client can send via a single connection before blocking."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "reconnectBackoff",
              "displayName": "Reconnect Backoff Time (ms)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The amount of time to wait before attempting to reconnect to a given host."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "reconnectBackoffMax",
              "displayName": "Reconnect Backoff Max Time (ms)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The maximum amount of time in milliseconds to wait when reconnecting to a broker that has repeatedly failed to connect."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "retryBackoff",
              "displayName": "Retry Backoff Time (ms)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The amount of time, in milliseconds, to wait before attempting to retry a failed request to a given topic partition."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "metadataMaxAge",
              "displayName": "Metadata Max Age (ms)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The period of time, in milliseconds, after which you should refresh metadata even if there was no partition leadership changes to proactively discover any new brokers or partitions."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "metadataFetchTimeout",
              "displayName": "Metadata Fetch Timeout (ms)",
              "inputType": "stringOrExpression",
              "defaultValue": "",
              "required": "false",
              "helpTip":"The maximum amount of time, in milliseconds, to block and wait for the metadata fetch to succeed before throwing an exception to the client."
            }
          },
          {
            "type": "attribute",
            "value": {
              "name": "metricReporters",
              "displayName": "Metric Reporters",
              "inputType": "stringOrExpression",
